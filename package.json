{
  "name": "vite-plugin-fakery",
  "version": "1.0.0alpha",
  "description": "Vite plugin to mock frontend APIs with realistic data from Faker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "vite",
    "plugin",
    "faker",
    "mock",
    "api"
  ],
  "author": "Caleb Pierce - https://calebpierce.dev",
  "license": "MIT",
  "peerDependencies": {
    "@faker-js/faker": "^8.0.2",
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
