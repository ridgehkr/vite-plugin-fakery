{
  "name": "vite-plugin-fakery",
  "version": "1.0.4",
  "description": "Vite plugin to mock frontend APIs with realistic data from Faker",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prepare": "husky"
  },
  "keywords": [
    "vite",
    "plugin",
    "faker",
    "mock",
    "api"
  ],
  "author": "Caleb Pierce - https://calebpierce.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ridgehkr/vite-plugin-fakery.git"
  },
  "bugs": {
    "url": "https://github.com/vitejs/vite-plugin-react/issues"
  },
  "homepage": "https://vite-plugin-fakery.netlify.app",
  "peerDependencies": {
    "@faker-js/faker": "^8.0.2",
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "module": "^1.2.5",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.2"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
